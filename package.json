{
  "name": "react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://username.github.io/Coca",
  "scripts": {
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "vite build",
    "biome:check": "npx biome check --write .",
    "biome:apply": "npx biome check --apply --write .",
    "biome:unsafe": "npx biome check --unsafe --write .",
    "preview": "vite preview",
    "watchStyles": "node frontend/src/utils/buildStyles/buildStyles.js",
    "generate": "plop",
    "server": "node backend/src/server.js",
    "start": "kill-port 5000 5173 && concurrently -n \"DEV,SERVER,WATCH_STYLES\" --prefix-colors \"blue.bold,magenta.bold,green.bold\" --kill-others-on-fail --success first --restart-tries 3 \"npm run dev\" \"npm run server\" \"npm run watchStyles\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tanstack/react-query": "^5.68.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "axios": "^1.8.3",
    "chokidar": "^4.0.3",
    "classnames": "^2.5.1",
    "compression": "^1.8.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "cssnano": "^7.0.6",
    "csurf": "^1.10.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "framer-motion": "^11.15.0",
    "gh-pages": "^6.2.0",
    "globals": "^15.9.0",
    "kill-port": "^2.0.1",
    "pg": "^8.13.3",
    "plop": "^4.0.1",
    "postcss": "^8.4.47",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-pxtorem": "^6.1.0",
    "postcss-sort-media-queries": "^5.2.0",
    "react-intersection-observer": "^9.13.1",
    "react-router-dom": "^7.1.1",
    "react-swipeable": "^7.0.1",
    "reactcci": "^1.14.0",
    "rome": "^12.1.3",
    "sass": "^1.80.4",
    "sweetalert2": "^11.17.2",
    "vite": "^5.4.8"
  }
